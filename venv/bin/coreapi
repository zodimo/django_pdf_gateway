#!/home/jaco/IdeaProjects/django_pdf_gateway/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'coreapi-cli==1.0.9','console_scripts','coreapi'
__requires__ = 'coreapi-cli==1.0.9'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('coreapi-cli==1.0.9', 'console_scripts', 'coreapi')()
    )
